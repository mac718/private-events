<h1><%= @user.name %></h1>

<h2>Created Events</h2>
  <% if @created_events %>
  <ol>
    <% @created_events.each do |event| %>
      <li><%= link_to "#{event.title}: #{event.description}", event_path(event.id) %></li>
    <% end %>
  </ol>
  <% end %>

<h2>Upcoming Events:</h2>
<% if @upcoming_events %>
  <ol>
    <% @upcoming_events.each do |event| %>
      <li><%= link_to "#{event.title}: #{event.description}", event_path(event.id) %></li>
    <% end %>
  </ol>
<% end %>

<h2>Previously Attended Events:</h2>
<% if @prev_events %>
  <ol>
    <% @prev_events.each do |event| %>
      <li><%= link_to "#{event.title}: #{event.description}", event_path(event.id) %></li>
    <% end %>
  </ol>
<% end %>